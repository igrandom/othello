release: MAIN.OBJ BACKGR.OBJ DRAWC.OBJ DRAWR.OBJ PRINTG.OBJ DRAWBW.OBJ DRAWEM.OBJ MOUSRDR.OBJ PRINTLB.OBJ STARTG.OBJ
	ML /W3 /WX BACKGR.OBJ DRAWC.OBJ DRAWR.OBJ PRINTG.OBJ DRAWBW.OBJ DRAWEM.OBJ MOUSRDR.OBJ PRINTLB.OBJ STARTG.OBJ MAIN.OBJ /Fe OTHELLO.EXE

debug: MAIND.OBJ BACKGRD.OBJ DRAWCD.OBJ DRAWRD.OBJ PRINTGD.OBJ DRAWBW.OBJ DRAWEMD.OBJ MOUSRDRD.OBJ PRINTLBD.OBJ STARTGD.OBJ
	ML /Zi /W3 /WX BACKGRD.OBJ DRAWCD.OBJ DRAWRD.OBJ PRINTGD.OBJ DRAWBW.OBJ DRAWEMD.OBJ MOUSRDRD.OBJ PRINTLBD.OBJ STARTGD.OBJ MAIND.OBJ /Fe MAIND.EXE

MAIN.OBJ: MAIN.ASM
	ML /c /W3 /WX /FoMAIN.OBJ /Ta MAIN.ASM
    
MAIND.OBJ: MAIN.ASM
	ML /c /Zi /W3 /WX /FoMAIND.OBJ /Ta MAIN.ASM

BACKGR.OBJ: BACKGR.ASM
	ML /c /W3 /WX /FoBACKGR.OBJ /Ta BACKGR.ASM
	
BACKGRD.OBJ: BACKGR.ASM
	ML /c /Zi /W3 /WX /FoBACKGRD.OBJ /Ta BACKGR.ASM
	
DRAWC.OBJ: DRAWC.ASM
	ML /c /W3 /WX /FoDRAWC.OBJ /Ta DRAWC.ASM
	
DRAWCD.OBJ: DRAWC.ASM
	ML /c /Zi /W3 /WX /FoDRAWCD.OBJ /Ta DRAWC.ASM
	
DRAWR.OBJ: DRAWR.ASM
	ML /c /W3 /WX /FoDRAWR.OBJ /Ta DRAWR.ASM
	
DRAWRD.OBJ: DRAWR.ASM
	ML /c /Zi /W3 /WX /FoDRAWRD.OBJ /Ta DRAWR.ASM

PRINTG.OBJ: PRINTG.ASM
	ML /c /W3 /WX /FoPRINTG.OBJ /Ta PRINTG.ASM
	
PRINTGD.OBJ: PRINTG.ASM
	ML /c /Zi /W3 /WX /FoPRINTGD.OBJ /Ta PRINTG.ASM

DRAWBW.OBJ: DRAWBW.ASM
	ML /c /W3 /WX /FoDRAWBW.OBJ /Ta DRAWBW.ASM
	
DRAWBWD.OBJ: DRAWBW.ASM
	ML /c /Zi /W3 /WX /FoDRAWBWD.OBJ /Ta DRAWBW.ASM

DRAWEM.OBJ: DRAWEM.ASM
	ML /c /W3 /WX /FoDRAWEM.OBJ /Ta DRAWEM.ASM
	
DRAWEMD.OBJ: DRAWEM.ASM
	ML /c /Zi /W3 /WX /FoDRAWEMD.OBJ /Ta DRAWEM.ASM

MOUSRDR.OBJ: MOUSRDR.ASM
	ML /c /W3 /WX /FoMOUSRDR.OBJ /Ta MOUSRDR.ASM
	
MOUSRDRD.OBJ: MOUSRDR.ASM
	ML /c /Zi /W3 /WX /FoMOUSRDRD.OBJ /Ta MOUSRDR.ASM

PRINTLB.OBJ: PRINTLB.ASM
	ML /c /W3 /WX /FoPRINTLB.OBJ /Ta PRINTLB.ASM
	
PRINTLBD.OBJ: PRINTLB.ASM
	ML /c /Zi /W3 /WX /FoPRINTLBD.OBJ /Ta PRINTLB.ASM


STARTG.OBJ: STARTG.ASM
	ML /c /W3 /WX /FoSTARTG.OBJ /Ta STARTG.ASM
	
STARTGD.OBJ: STARTG.ASM
	ML /c /Zi /W3 /WX /FoSTARTGD.OBJ /Ta STARTG.ASM





clean:
	del *.EXE
	del *.OBJ

cv: debug
	cv /s /43 MAIND.EXE
