release: N.OBJ B.OBJ C.OBJ R.OBJ P.OBJ D.OBJ M.OBJ Q.OBJ L.OBJ S.OBJ
	ML /W3 /WX B.OBJ C.OBJ R.OBJ P.OBJ D.OBJ M.OBJ Q.OBJ L.OBJ S.OBJ N.OBJ /Fe OTHELLO.EXE

debug: ND.OBJ BD.OBJ CD.OBJ RD.OBJ PD.OBJ DD.OBJ MD.OBJ QD.OBJ LD.OBJ SD.OBJ
	ML /Zi /W3 /WX BD.OBJ CD.OBJ RD.OBJ PD.OBJ D.OBJ MD.OBJ QD.OBJ LD.OBJ SD.OBJ ND.OBJ /Fe ND.EXE

N.OBJ: N.ASM
	ML /c /W3 /WX /FoN.OBJ /Ta N.ASM
    
ND.OBJ: N.ASM
	ML /c /Zi /W3 /WX /FoND.OBJ /Ta N.ASM

B.OBJ: B.ASM
	ML /c /W3 /WX /FoB.OBJ /Ta B.ASM
	
BD.OBJ: B.ASM
	ML /c /Zi /W3 /WX /FoBD.OBJ /Ta B.ASM
	
